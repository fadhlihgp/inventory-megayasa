	<queryString>
		<![CDATA[select a.full_name, a.username, a.email, r.name, 
IFNULL(a.employee_id, '-') as employee_id, 
a.last_login,
CASE
   WHEN a.is_active = 1 THEN 'Aktif'
   ELSE 'Keluar'
END As status
from account a
join role r on r.id = a.role_id]]>
